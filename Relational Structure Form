Relational Structure Form:

Account(A_ACCOUNTID, a_username, a_password)

Account_For_Users(A_ACCOUNTID, U_USERID)
	foreign key (a_accountid) references Account
	foreign key (u_userid) references Users

Users(U_USERID, u_firstname, u_lastname, u_phonenumber, u_credit 

Users_Vehicle(U_USERID, U_LICENSEPLATENUM) 
	foreign key(u_userid) references Users

Users_Use_Meter(U_USERID, P_METERID)
	foreign key (u_userid) references Users
	foreign key (p_meterid) references Parking_Meter

Parking_Meter(P_METERID, p_currentuser, p_previoususer, p_usestoday, p_isinuse

Parking_Time(P_METERID, P_TIMEREMAINING)
	foreign key (p_meterid) references Parking_Meter

Security_Personnel(S_WORKERID, s_firstname, s_lastname, s_phonenumber

Security_Schedule(S_WORKERID, S_SCHEDULE)
	foreign key (S_WORKERID) references Security_Personnel
	
Security_Tickets_Users(U_USERID, S_WORKERID)
	foreign key (u_userid) references Users
	foreign key (s_workerid) references Security_Personnel
	
Security_Checks_Meter(S_WORKERID, P_METERID)
	foreign key (s_workerid) references Security_Personnel
	foreign key (p_meterid) references Parking_Meter
	
